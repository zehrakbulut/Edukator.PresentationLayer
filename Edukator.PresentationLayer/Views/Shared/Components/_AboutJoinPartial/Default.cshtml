    <div class="container py-md-5 py-2">
        <div class="w3l-project-in">
            <div class="row">
                <div class="col-lg-7">
                    <div class="bottom-info">
                        @foreach (var item in Model)
                        {
                        <div class="header-section pr-lg-5">
                            <h5 class="title-subw3hny mb-3">@item.Title1</h5>
                            <h3 class="title-w3l mb-3">
                                <span>@item.Title2</span>
                            </h3>
                            <p>
                               @item.Description
                            </p>
                        </div>
                        }
                    </div>
                </div>
                <div class="col-lg-5 align-self mt-lg-0 mt-sm-5 mt-4">
                    <div class="d-sm-flex justify-content-end">
                        <a href="#" class="btn btn-primary btn-style mr-sm-2"> Öğretmen</a>
                        <a href="#" class="btn btn-secondary btn-style"> Öğrenci </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>